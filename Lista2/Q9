package main

import "fmt"

func main() {
	var x int
	fmt.Println("Digite o valor da vendA")
	fmt.Scan(&x)
	if x < 10 && x > 0 {
		fmt.Println(1.7 * float64(x))
	} else if x >= 10 && x < 30 {
		fmt.Println(1.5 * float64(x))
	} else if x >= 30 && x < 50 {
		fmt.Println(1.4 * float64(x))
	} else if x >= 50 {
		fmt.Println(1.3 * float64(x))
	}

}
